<h1><%= user.name %>个人主页</h1>
<div class="content-mid">
   <% if(papers){
	papers.forEach(function(paper,index){%>
	   	<div class="section">
			<h2><a href="/paper?author=<%= paper.author%>&title=<%= paper.title%>"><%= paper.title%></a></h2>
			<%- paper.content%>
			<ul>
				<li>作者：<a href='/user?name=<%=paper.author%>'><%=paper.author%></a></li>
				<li>日期：<%= paper.time%></li>
				<li>阅读：<%= paper.pv%></li>
				<li>评论：<%= paper.comments.length%></li>
			</ul>
		</div>
   <%})} %>
	<div class="devide">
		<ul>
			<li><a href="">上一页</a></li>
			<li><a href="">下一页</a></li>
		</ul>
	</div>
</div>